{
  "name": "webpack-experiments",
  "version": "1.0.0",
  "description": "Webpack Experiments",
  "main": "src/index.js",
  "scripts": {
    "remove-dist": "rm -rf ./dist",
    "test": "jest --config='jest.config.js'",
    "dev": "npm run remove-dist && npx webpack-dev-server --mode=development --config webpack.dev.config.js",
    "dev-watch": "nodemon --watch package.json --watch tsconfig.json --watch webpack.base.config.js --watch webpack.dev.config.js --exec \"npm run dev\"",
    "check-types": "tsc",
    "build": "npm run remove-dist && npm run check-types && npm run test && webpack --mode=production --config webpack.prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sean-Tay/webpack-experiments.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Sean Tay",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sean-Tay/webpack-experiments/issues"
  },
  "homepage": "https://github.com/Sean-Tay/webpack-experiments#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@types/jest": "^26.0.7",
    "@types/react": "^16.9.34",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.4.2",
    "css-modules-typescript-loader": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "purecss": "^1.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "@babel/runtime": "^7.9.6"
  }
}
