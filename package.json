{
  "name": "webpack-experiments",
  "version": "1.0.0",
  "description": "Webpack Experiments",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon --watch package.json --watch webpack.config.js --watch webpack.dev.config.js --exec \"rm -rf ./dist && npx webpack-dev-server --mode=development --config webpack.dev.config.js\"",
    "check-types": "tsc",
    "build": "rm -rf ./dist && npm run check-types && webpack --mode=production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sean-Tay/webpack-experiments.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Sean Tay",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sean-Tay/webpack-experiments/issues"
  },
  "homepage": "https://github.com/Sean-Tay/webpack-experiments#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.4.2",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "html-webpack-plugin": "^4.0.4",
    "nodemon": "^2.0.2",
    "style-loader": "^1.1.3",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@babel/preset-react": "^7.9.4",
    "@types/react": "^16.9.34",
    "node-sass": "^4.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass-loader": "^8.0.2"
  }
}
